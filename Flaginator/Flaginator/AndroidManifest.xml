<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.flaginator"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="16" />

    <application
        android:icon="@drawable/icon_transparent"
        android:label="@string/app_name"
        android:debuggable="true" 
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Flaginator"
            android:configChanges="orientation|keyboardHidden"
            android:label="@string/title_activity_flaginator" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".About"
            android:label="@string/title_activity_about"
            android:configChanges="orientation|keyboardHidden"
            android:theme="@android:style/Theme.Dialog" >
        </activity>
        <activity
            android:name=".Settings2"
            android:label="@string/title_activity_settings" >
        </activity>
        <activity
            android:name=".StartCapital"
            android:label="@string/title_activity_start_capital" >
        </activity>
        <activity
            android:name=".StartFlag"
            android:label="@string/title_activity_start_flag" >
        </activity>
        <activity
            android:name=".Chart"
            android:label="@string/title_activity_chart" >
        </activity>
    </application>

</manifest>